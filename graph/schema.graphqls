# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
    id: ID!
    name: String!
}

input CreateUserInput {
    username: String!
    email: String!
    password: String!
    prefecture: Int!
    introduction: String!
    bloodType: Int!
}

type Query {
    getUser(input: ID!): User!
}

type Mutation {
    createUser(input: CreateUserInput!) : User
}
